	$(function(){
	$("#btnAjax").click(
			function(){
			$.ajax({
  			type: 'POST',
  			url: "$!basePath/meet/roomarrController/getajax",
  			success: function(data){
			var result = jQuery.parseJSON(data);   
			        if(result.success == true){
			       		top.Notiy.success("Ajax成功!");		        		
			        }else{
			        	top.Dialog.alert("提示",result.error,"error");
			        }
				}
			}
		)
	}		 
)
})
	
	</script>
